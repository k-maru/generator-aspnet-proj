{
    "version": "1.0.0-*",
    "webroot": "wwwroot",
    "exclude": [
        "wwwroot"
    ],
    "dependencies": {
        "Microsoft.AspNet.Hosting": "1.0.0-beta1",<% if(server.indexOf('weblistener') > -1) { %>
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta1",<% } if(server.indexOf('kestrel') > -1) { %>
        "kestrel": "1.0.0-*",<% } if(server.indexOf('iis') > -1) { %>
        "Microsoft.AspNet.Server.IIS": "1.0.0-beta1",<% } %>
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta1"
    },
    "commands": { <% if(server.indexOf('weblistener') > -1) { %>
        "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5001",<% } if(server.indexOf('kestrel') > -1) { %>
        "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5001" <% } %>
    },
    "frameworks": {
        "aspnet50": {},
        "aspnetcore50": {}
    }
}
